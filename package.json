{
  "name": "my-backend-stock",
  "version": "0.0.1",
  "description": "my-backend-stock",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "18 || 20 || 22"
  },
  "scripts": {
    "build": "lb-tsc",
    "start": "node -r source-map-support/register dist/index.js",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t my-backend-stock .",
    "docker:run": "docker run -p 3000:3000 -d my-backend-stock",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Ricardo <rricardo.23.11.2022@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^11.0.7",
    "@loopback/authentication-jwt": "^0.15.7",
    "@loopback/boot": "^7.0.6",
    "@loopback/core": "^6.1.3",
    "@loopback/repository": "^7.0.6",
    "@loopback/rest": "^14.0.6",
    "@loopback/rest-explorer": "^7.0.7",
    "@loopback/security": "^0.11.7",
    "@loopback/service-proxy": "^7.0.6",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "loopback-connector-mongodb": "github:loopbackio/loopback-connector-mongodb",
    "moment": "^2.30.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^11.0.6",
    "@loopback/eslint-config": "^15.0.4",
    "@loopback/testlab": "^7.0.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^16.18.101",
    "eslint": "^8.57.0",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.2"
  }
}
